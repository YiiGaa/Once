[
    {
        "fileName": "SampleService.java",
        "path":"/yiigaa/once/service",
        "document": {
            "templ":"DOCUMENT-SERVICE",
            "name": "SampleService",
            "group":"yiigaa",
            "projectname":"once",
            "function": [
                {
                    "templ":"FUNCTION-NORMAL",
                    "functionName":"addData",
                    "module":[
                        {
                            "templ":"MODULE-DAO",
                            "param_templ":"PARAM-Hash",
                            "param":[
                                {
                                    "control":"insert",
                                    "form":"t_test"
                                }
                            ]
                        }
                    ]
                }, {
                    "templ":"FUNCTION-NORMAL",
                    "functionName":"addDataBatch",
                    "module":[
                        {
                            "templ":"MODULE-DAO",
                            "param_templ":"PARAM-Hash",
                            "param":[
                                {
                                    "control":"batchInsert",
                                    "targetKey":"temp_insert",
                                    "query":"INSERT INTO t_test (test_id, test_name) VALUES #'@test_id@', '@test_name@'#"
                                }
                            ]
                        }
                    ]
                },{
                    "templ":"FUNCTION-NORMAL",
                    "functionName":"getData",
                    "module":[
                        {
                            "templ":"MODULE-DAO",
                            "param_templ":"PARAM-Hash",
                            "param":[
                                {
                                    "control":"select",
                                    "isAsResult":"true",
                                    "per_page":"3",
                                    "query":"select * from t_test where test_id='@test_id@'"
                                }
                            ]
                        }
                    ]
                },{
                    "templ":"FUNCTION-NORMAL",
                    "functionName":"getDataAll",
                    "module":[
                        {
                            "templ":"MODULE-DAO",
                            "param_templ":"PARAM-Hash",
                            "param":[
                                {
                                    "control":"select",
                                    "form":"t_test",
                                    "isAsResult":"true",
                                    "isAllGet":"true"
                                }
                            ]
                        }
                    ]
                }
            ]
        }
    }
]
