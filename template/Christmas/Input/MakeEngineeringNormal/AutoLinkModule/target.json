{
    "configure":{
        "@root@":"..",
        "@ConModuleRoot@":"/src/main/java/com/yiigaa/once/controllermodule",
        "@SerModuleRoot@":"/src/main/java/com/yiigaa/once/servicemodule",
        "@DaoRoot@":"/src/main/java/com/yiigaa/once/dao",
        "@TargetTemplateRoot@":"/Christmas/Template",
        "@TemplateRoot@":"/Christmas/Template/AutoLinkModule"
    },
    "control":{
        "generatePath":"@root@",
        "enginneringPath":"@root@",
        "directMerger":[
            {
                "targetPath":"@ConModuleRoot@/ErrorCodes.java",
                "sourcePath":[
                    {"rootPath":"@TemplateRoot@","contain":"ControllerErrorCodes.tmpl","except":"_bak_","filter":"filter_FileHead"},
                    {"rootPath":"@ConModuleRoot@","contain":".java","except":"ControllerModule.java,ErrorCodes.java,Link.java,Config.java,_bak_","filter":"filter_ErrorCodes"},
                    {"rootPath":"@TemplateRoot@","contain":"ControllerErrorCodes.tmpl","except":"_bak_","filter":"filter_FileTail"}
                ]
            },{
                "targetPath":"@ConModuleRoot@/ControllerModule.java",
                "sourcePath":[
                    {"rootPath":"@TemplateRoot@","contain":"ControllerModule.tmpl","except":"_bak_","filter":"filter_FileHead"},
                    {"rootPath":"@ConModuleRoot@","contain":".java","except":"ControllerModule.java,ErrorCodes.java,Link.java,Config.java,_bak_","filter":"filter_LinkpackageImport"},
                    {"rootPath":"@TemplateRoot@","contain":"ControllerModule.tmpl","except":"_bak_","filter":"filter_FileBody"},
                    {"rootPath":"@ConModuleRoot@","contain":".java","except":"ControllerModule.java,ErrorCodes.java,Link.java,Config.java,_bak_","filter":"filter_LinkObjectImport"},
                    {"rootPath":"@TemplateRoot@","contain":"ControllerModule.tmpl","except":"_bak_","filter":"filter_FileTail"}
                ]
            }, {
                "targetPath":"@SerModuleRoot@/ErrorCodes.java",
                "sourcePath":[
                    {"rootPath":"@TemplateRoot@","contain":"ServiceErrorCodes.tmpl","except":"_bak_","filter":"filter_FileHead"},
                    {"rootPath":"@DaoRoot@","contain":"DataBaseDao.java","except":"_bak_","filter":"filter_ErrorCodes"},
                    {"rootPath":"@SerModuleRoot@","contain":".java","except":"ServiceModule.java,ErrorCodes.java,Link.java,Config.java,_bak","filter":"filter_ErrorCodes"},
                    {"rootPath":"@TemplateRoot@","contain":"ServiceErrorCodes.tmpl","except":"_bak_","filter":"filter_FileTail"}
                ]
            },{
                "targetPath":"@SerModuleRoot@/ServiceModule.java",
                "sourcePath":[
                    {"rootPath":"@TemplateRoot@","contain":"ServiceModule.tmpl","except":"_bak_","filter":"filter_FileHead"},
                    {"rootPath":"@SerModuleRoot@","contain":".java","except":"ServiceModule.java,ErrorCodes.java,Link.java,Config.java,_bak_","filter":"filter_LinkpackageImport"},
                    {"rootPath":"@TemplateRoot@","contain":"ServiceModule.tmpl","except":"_bak_","filter":"filter_FileBody"},
                    {"rootPath":"@SerModuleRoot@","contain":".java","except":"ServiceModule.java,ErrorCodes.java,Link.java,Config.java,_bak_","filter":"filter_LinkObjectImport"},
                    {"rootPath":"@TemplateRoot@","contain":"ServiceModule.tmpl","except":"_bak_","filter":"filter_FileTail"}
                ]
            },{
                "targetPath":"/build.gradle",
                "sourcePath":[
                    {"rootPath":"@TemplateRoot@","contain":"gradle.tmpl","except":"_bak_","filter":"filter_FileHead"},
                    {"rootPath":"@ConModuleRoot@","contain":".java","except":"ControllerModule.java,ErrorCodes.java,Link.java,Config.java,_bak_","filter":"filter_GradleImport"},
                    {"rootPath":"@SerModuleRoot@","contain":".java","except":"ServiceModule.java,ErrorCodes.java,Link.java,Config.java,_bak_","filter":"filter_GradleImport"},
                    {"rootPath":"@DaoRoot@","contain":"DataBaseDao.java","except":"_bak_","filter":"filter_GradleImport"},
                    {"rootPath":"@TemplateRoot@","contain":"gradle.tmpl","except":"_bak_","filter":"filter_FileTail"}
                ]
            },{
                "targetPath":"@TargetTemplateRoot@/Controller/module.templ",
                "sourcePath":[
                    {"rootPath":"@TemplateRoot@","contain":"param.templ"},
                    {"rootPath":"@TemplateRoot@","contain":"module.templ"},
                    {"rootPath":"@ConModuleRoot@","contain":"Templates.tmpl","except":"_bak_","filter":"filter_CodeTemplate"}
                ]
            },{
                "targetPath":"@TargetTemplateRoot@/Service/module.templ",
                "sourcePath":[
                    {"rootPath":"@TemplateRoot@","contain":"param.templ"},
                    {"rootPath":"@SerModuleRoot@","contain":"Templates.tmpl","except":"_bak_","filter":"filter_CodeTemplate"},
                    {"rootPath":"@DaoRoot@","contain":"Templates.tmpl","except":"_bak_","filter":"filter_CodeTemplate"}
                ]
            },{
                "targetPath":"/config/application.properties",
                "sourcePath":[
                    {"rootPath":"@TemplateRoot@","contain":"properties.tmpl","except":"_bak_","filter":"filter_FileTail"},
                    {"rootPath":"@ConModuleRoot@","contain":"Templates.tmpl","except":"_bak_","filter":"filter_propertiesTemplate"},
                    {"rootPath":"@SerModuleRoot@","contain":"Templates.tmpl","except":"_bak_","filter":"filter_propertiesTemplate"},
                    {"rootPath":"@DaoRoot@","contain":"Templates.tmpl","except":"_bak_","filter":"filter_propertiesTemplate"}
                ]
            },{
                "targetPath":"/config/application-local.properties",
                "sourcePath":[
                    {"rootPath":"@TemplateRoot@","contain":"properties.tmpl","except":"_bak_","filter":"filter_FileHead"},
                    {"rootPath":"@ConModuleRoot@","contain":"Templates.tmpl","except":"_bak_","filter":"filter_propertiesLocalTemplate"},
                    {"rootPath":"@SerModuleRoot@","contain":"Templates.tmpl","except":"_bak_","filter":"filter_propertiesLocalTemplate"},
                    {"rootPath":"@DaoRoot@","contain":"Templates.tmpl","except":"_bak_","filter":"filter_propertiesLocalTemplate"}
                ]
            }
        ]
    }
}