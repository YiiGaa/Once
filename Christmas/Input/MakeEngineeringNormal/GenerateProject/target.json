{
    "configure":{
        "@targetGroup@":"yiigaa",
        "@targetFile@":"../@@-once-backend",
        "@codeRoot@":"/src/main/java/com",
        "@testRoot@":"/src/test/java/com"
    },
    "control":{
        "enginneringPath":"../template",
        "generatePath":"@targetFile@",
        "directCopy":[
            {"sourcePath":"/Christmas","targetPath":"/Christmas"},
            {"sourcePath":"/config","targetPath":"/config"},
            {"sourcePath":"/gradle","targetPath":"/gradle"},
            {"sourcePath":"/.gitignore","targetPath":"/"},
            {"sourcePath":"/build.gradle","targetPath":"/"},
            {"sourcePath":"/gradlew","targetPath":"/"},
            {"sourcePath":"/gradlew.bat","targetPath":"/"},
            {"sourcePath":"/settings.gradle","targetPath":"/"},
            {"sourcePath":"@codeRoot@/yiigaa/once/controller", "targetPath":"@codeRoot@/@targetGroup@/@@/controller"},
            {"sourcePath":"@codeRoot@/yiigaa/once/controllercommon", "targetPath":"@codeRoot@/@targetGroup@/@@/controllercommon"},
            {"sourcePath":"@codeRoot@/yiigaa/once/controllermodule", "targetPath":"@codeRoot@/@targetGroup@/@@/controllermodule"},
            {"sourcePath":"@codeRoot@/yiigaa/once/dao", "targetPath":"@codeRoot@/@targetGroup@/@@/dao"},
            {"sourcePath":"@codeRoot@/yiigaa/once/service", "targetPath":"@codeRoot@/@targetGroup@/@@/service"},
            {"sourcePath":"@codeRoot@/yiigaa/once/servicecommon", "targetPath":"@codeRoot@/@targetGroup@/@@/servicecommon"},
            {"sourcePath":"@codeRoot@/yiigaa/once/servicemodule", "targetPath":"@codeRoot@/@targetGroup@/@@/servicemodule"},
            {"sourcePath":"@codeRoot@/yiigaa/once/ServletInitializer.java","targetPath":"@codeRoot@/@targetGroup@/@@"}
        ],
        "directMerger":[
            {
                "targetPath":"@codeRoot@/@targetGroup@/@@/@@Application.java",
                "sourcePath":[
                    {"rootPath":"@codeRoot@/yiigaa/once","contain":"onceApplication.java"}
                ]
            },
            {
                "targetPath":"@testRoot@/@targetGroup@/@@/@@ApplicationTests.java",
                "sourcePath":[
                    {"rootPath":"@testRoot@/yiigaa/once","contain":"onceApplicationTests.java"}
                ]
            }
        ],
        "targetReplace":[
            {
                "targetPath":{"rootPath":"/src","contain":".java"},
                "replaceStr":{
                    "yiigaa":"@targetGroup@",
                    "once":"@@"
                }
            },
            {
                "targetPath":{"rootPath":"/","contain":"settings.gradle"},
                "replaceStr":{
                    "yiigaa":"@targetGroup@",
                    "once":"@@"
                }
            },
            {
                "targetPath":{"rootPath":"/Christmas/Input","contain":".json"},
                "replaceStr":{
                    "yiigaa":"@targetGroup@",
                    "once":"@@"
                }
            },
            {
                "targetPath":{"rootPath":"/Christmas/Template","contain":".t"},
                "replaceStr":{
                    "yiigaa":"@targetGroup@",
                    "once":"@@"
                }
            },
            {
                "targetPath":{"rootPath":"/config"},
                "replaceStr":{
                    "yiigaa":"@targetGroup@",
                    "once":"@@"
                }
            }
        ]
    }
}
